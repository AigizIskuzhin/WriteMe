@using Website.ViewModels
@using Website.ViewModels.Profile
@model Website.ViewModels.Profile.PostViewModel
@{
    Layout = null;
}
@if (Model != null)
{
    if(Model is UserPostViewModel userPost)
        Html.RenderPartial("UserPostView",userPost);
    else
        Html.RenderPartial("SystemPostView", Model as SystemPostViewModel);
}