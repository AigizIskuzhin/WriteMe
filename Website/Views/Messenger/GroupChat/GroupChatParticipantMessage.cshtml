@model Database.DAL.Entities.Chat.GroupChat.GroupChatParticipantMessage
@{
    Layout = null;
}
<li id="message-@Model.Id" class="message" data-group-chat-message-id="@Model.Id" data-message-owner-id="@Model.Sender.Id">
    <div class="message_sender">
        @Model.Sender.User.Name
    </div>
    <div class="message-content">
        @Model.Text
    </div>
    <div class="message-sent-time">
        @Model.SentDateTime
    </div>
</li>